help = gen tag and write bench score to meta db
abbr = bench.rec
trivial = 1

flow = [[bench.workload]].gen-tag : [[bench.workload]].record : bench.lib.record

[arg]
tag-add-from-keys|add-keys|add-key = ''
tag-from-keys|keys = bench.workload,bench.workload.tag,tidb.version

[arg2env]
bench.tag-from-keys = tag-from-keys
bench.tag-add-from-keys = tag-add-from-keys

[env]
bench.workload = read
