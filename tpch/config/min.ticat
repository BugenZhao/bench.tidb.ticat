help = setup TPCH config: use minimum data scale

[arg]
load-threads|load-thread|ld = 1
threads|thread|thrd|t|T = 1
scale-factor|scale|sf = 1
duration|dur = 1m
with-tiflash|tiflash = true

[arg2env]
bench.tpch.load.threads = load-threads
bench.tpch.threads = threads
bench.tpch.scale-factor = scale-factor
bench.tpch.duration = duration
bench.tpch.tiflash = with-tiflash

[val2env]
bench.workload = tpch
